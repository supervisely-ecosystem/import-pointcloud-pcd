    "task_location": "workspace_tasks",
    "icon_background": "#FFFFFF",
    "modal_template": "src/modal.html",
    "modal_template_state": {
      "remove_source": true,
      "project_name": ""
    },
    "icon": "",
    "icon_cover": true,

    "headless": true,
    "instance_version": "6.4.58",

    "context_menu": {
        "target": ["files_folder"]
    },
    "poster": ""
}