{
    "name": "Import pointclouds PCD",
    "type": "app",
    "version": "2.0.0",
    "categories": [
      "import",
      "pointclouds"
    ],
    "description": "Import pointclouds without annotations in .pcd format from Team Files",
    "docker_image": "supervisely/base-py-sdk:6.23.1",
    "entrypoint": "python -m uvicorn src.main:g.app --host 0.0.0.0 --port 8000",
